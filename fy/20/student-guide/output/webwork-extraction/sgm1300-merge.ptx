    <server-url hint="no" solution="yes">http://localhost/webwork2/html2xml?courseID=anonymous&amp;userID=anonymous&amp;password=anonymous&amp;course_password=anonymous&amp;answersSubmitted=0&amp;displayMode=MathJax&amp;outputformat=simple&amp;problemSeed=9&amp;sourceFilePath=Library/FortLewis/Basic-skills-pretest/Skill-Assessment-06.pg</server-url>

    <server-url hint="no" solution="no">http://localhost/webwork2/html2xml?courseID=anonymous&amp;userID=anonymous&amp;password=anonymous&amp;course_password=anonymous&amp;answersSubmitted=0&amp;displayMode=MathJax&amp;outputformat=simple&amp;problemSeed=9&amp;sourceFilePath=Library/FortLewis/Basic-skills-pretest/Skill-Assessment-06.pg</server-url>

